{
  "name": "obs-tablet-remote",
  "version": "0.0.0",
  "description": "OBS Remote interface that's pretty okay I suppose",
  "homepage": "http://t2t2.github.io/obs-tablet-remote/",
  "repository": "https://github.com/t2t2/obs-tablet-remote",
  "author": "t2t2 <taavot@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vbuild",
    "dev": "vbuild dev",
    "lint": "xo"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "sanitize.css": "^5.0.0",
    "sha.js": "^2.4.8",
    "vue": "^2.2.6",
    "vuex": "^2.2.1",
    "vuex-persistedstate": "^1.3.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.3.2",
    "eslint": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "loader-utils": "^1.1.0",
    "node-sass": "^4.5.2",
    "postcss-flexbugs-fixes": "^3.0.0",
    "sass-loader": "^6.0.3",
    "vbuild": "^7.1.0",
    "xo": "^0.18.2"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "semicolon": false,
    "plugins": [
      "html"
    ],
    "extensions": [
      "vue"
    ],
    "settings": {
      "import/resolver": "webpack"
    },
    "overrides": [
      {
        "files": "src/store/**/{actions,getters,mutations}.js",
        "rules": {
          "import/prefer-default-export": 0
        }
      }
    ]
  }
}
