{
  "name": "obs-tablet-remote",
  "version": "0.0.0",
  "description": "OBS Remote interface that's pretty okay I suppose",
  "homepage": "http://t2t2.github.io/obs-tablet-remote/",
  "repository": "https://github.com/t2t2/obs-tablet-remote",
  "author": "t2t2 <taavot@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vbuild",
    "dev": "vbuild -d",
    "lint": "xo"
  },
  "dependencies": {
    "sanitize.css": "^4.1.0",
    "vue": "^2.2.1",
    "vuex": "^2.2.1",
    "vuex-persistedstate": "^1.3.0"
  },
  "devDependencies": {
    "babel-preset-env": "^0.0.9",
    "eslint": "^3.18.0",
    "eslint-import-resolver-webpack": "^0.7.0",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-import": "^2.2.0",
    "loader-utils": "^1.0.3",
    "node-sass": "^3.13.0",
    "postcss-flexbugs-fixes": "^2.0.0",
    "sass-loader": "^4.0.2",
    "vbuild": "^6.23.0",
    "webpack": "^2.1.0-beta.27",
    "xo": "^0.18.1"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "semicolon": false,
    "plugins": [
      "html"
    ],
    "extensions": [
      "vue"
    ],
    "settings": {
      "import/resolver": "webpack"
    },
    "overrides": [
      {
        "files": "**/*.vue",
        "rules": {
          "unicorn/no-abusive-eslint-disable": "off",
          "unicorn/filename-case": [
            "error",
            {
              "case": "pascalCase"
            }
          ]
        }
      }
    ]
  }
}
